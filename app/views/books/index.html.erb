<%= render 'book', book: @book, user: @user %>

<h2>books</h2>
<table class="null">
  <tr>
    <th colspan="1"></th>
    <th>Title</th>
    <th>Body</th>
  </tr>
  <% @books.each do |book| %>
  <tr>
    <td><%= image_tag book.user.get_profile_image(100,100) %></td>  
    <td><%= link_to "#{book.title}", book_path(book.id) %></td>
    <td><%= book.body %></td>
  </tr>
  <% end %>
</table>